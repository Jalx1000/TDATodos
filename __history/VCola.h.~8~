#ifndef VColaH
#define VColaH
//---------------------------------------------------------------------------
#include <iostream>
#include <string>
using namespace std;

class VCola
{
    typedef int tipo_elemento;
  private:
    int MAX = 100;
    tipo_elemento VC[MAX];
    int init;
    int Dir;
    int fin;
    int ini;
  public:
    VCola();
    bool vacia();
    void Poner(tipo_elemento E);
    void Sacar(tipo_elemento* E);
    tipo_elemento Primero();
};

#endif

